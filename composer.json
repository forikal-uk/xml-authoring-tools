{
    "name": "forikal-uk/xml-authoring-tools",
    "description": "PHP Console commands for managing xml files.",
    "type": "library",
    "license": "Apache-2.0",
    "bin": [
        "bin/console"
    ],
    "autoload": {
        "psr-4": {
            "Forikal\\Tools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Forikal\\Tools\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "symfony/console": "^3.4",
        "symfony/filesystem": "^3.4",
        "symfony/yaml": "^3.4"
    },
    "require-dev": {
        "symfony/var-dumper": "^3.4",
        "phpunit/phpunit": "^5.6"
    },
    "extra": {
        "branch-alias": null
    }
}
